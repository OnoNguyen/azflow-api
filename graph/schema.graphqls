package graph

type Todo struct {
	ID     string `json:"id"`
	Text   string `json:"text"`
	Done   bool   `json:"done"`
	UserID string `json:"userId"`
	User   *User  `json:"user"`
}

type User struct {
	ID       string  `json:"id"`
	UserName string  `json:"name"`
	Email    string  `json:"email"`
	Avatar   string  `json:"avatar"`
	Notes    []*Note `json:"notes"`
}
